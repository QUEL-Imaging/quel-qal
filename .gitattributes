# Raw 16-bit images
qal/data/**/*.tif filter=lfs diff=lfs merge=lfs -text
qal/data/**/*.tiff filter=lfs diff=lfs merge=lfs -text
qal/data/**/*.TIF filter=lfs diff=lfs merge=lfs -text

# Large image files
qal/data/**/*.png filter=lfs diff=lfs merge=lfs -text

# 3D models
qal/data/**/*.stl filter=lfs diff=lfs merge=lfs -text

# Scientific data
qal/data/**/*.npy filter=lfs diff=lfs merge=lfs -text
qal/data/**/*.bin filter=lfs diff=lfs merge=lfs -text
